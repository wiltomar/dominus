{
  "type": "postgres",
  "host": "localhost",
  "port": 5432,
  "username": "postgres",
  "password": "docker",
  "database": "catalog",
  "synchronize": false,
  "logging": false,
  "options": {
     "encrypt": true,
     "enableArithAbort": true,
     "trustedConnection": true,
     "cryptoCredentialsDetails": {
        "minVersion": "TLSv1"
      }
  },
  "entities": [
     "src/common/models/**/*.ts",
     "src/public/models/**/*.ts"
  ],
  "migrations": [
     "src/common/database/migrations/**/*.ts"
  ],
  "cli": {
     "migrationsDir": "src/common/database/migrations"
  }
}